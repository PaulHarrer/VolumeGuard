{
  "manifest_version": 2,
  "name": "Volume Guard",
  "version": "1.0",
  "description": "Allows you to edit the audio level per tab.",
  "permissions": ["tabs", "activeTab"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
